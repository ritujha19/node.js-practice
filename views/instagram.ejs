<%- include('includes/head.ejs') %>
<body>
    <h2>this page belongs to @ <%= Userdata.name %> </h2>
    <p>
        followers : <%= Userdata.followers %> &nbsp;&nbsp;&nbsp;&nbsp; followings : <%= Userdata.following %>
    </p>
    <hr />
    <div class="posts">
            <% for(let post of Userdata.posts) { %>
                <img src="<%= post.image %>" alt="image of user" />
                <p>
                    likes : <%= post.likes %>  &nbsp;&nbsp;&nbsp;&nbsp; comments : <%= post.comments %>
                </p>
            <% } %>
    </div>
</body>
</html>